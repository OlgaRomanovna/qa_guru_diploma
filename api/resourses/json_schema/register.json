{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "_id": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "__v": {
          "type": "integer"
        }
      },
      "required": [
        "_id",
        "email",
        "firstName",
        "lastName",
        "__v"
      ]
    },
    "token": {
      "type": "string"
    }
  },
  "required": [
    "user",
    "token"
  ]
}